Host *
  UseKeychain yes
  AddKeysToAgent yes

#Github (default)
  Host gh
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa

#Bitbucket (secondary)
  Host bb
  HostName bitbucket.org
  User git
  IdentityFile ~/.ssh/id_rsa

#AWS
  Host aws_cova_instance
  Hostname 18.138.162.138
  User ubuntu
  PubKeyAuthentication yes
  IdentityFile ~/.ssh/id_rsa_covaaws